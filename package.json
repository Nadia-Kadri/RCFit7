  "bugs": {
    "url": "https://github.com/Nadia-Kadri/RCFit7/issues"
  },
  "homepage": "https://github.com/Nadia-Kadri/RCFit7#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "google-maps-react": "^2.0.2",
    "if-env": "^1.0.4",
    "moment": "^2.24.0",
    "mongoose": "^5.9.2",
    "nodemailer": "^6.4.8",
    "passport": "^0.4.1",
    "passport-local-mongoose": "^6.0.1",
    "react-big-calendar": "^0.24.0"
